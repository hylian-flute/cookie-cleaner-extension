{
  "manifest_version": 3,
  "name": "Cookie Cleaner",
  "version": "1.0",
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "scripting",
    "activeTab",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
